{
  "version": 2,
  "builds": [
    {
      "src": "manage.py",
      "use": "@vercel/python",
      "config": {
        "pythonVersion": "3.8"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/manage.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "urine_strip_project.settings",
    "PYTHONUNBUFFERED": "1"
  },
  "buildCommand": "pip install -r requirements.txt",
  "devCommand": "python3 manage.py runserver"
}
